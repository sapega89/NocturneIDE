QTableCornerButton::section {
  background-color: #1b201b;
  border: 1px transparent #242424;
  border-radius: 0px;
}

/* ===================== QSlider ======================== */

QSlider::groove:horizontal {
  background-color: #3a3a3a;
  height: 6px;
  border-radius: 2px;
}

QSlider::sub-page:horizontal {
  background-color: #305830;
  height: 6px;

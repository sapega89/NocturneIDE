  /* VLine */
}

.QFrame[frameShape="0"] {
  border-radius: 4px;
  border: 1px transparent #6b7e6b;
}

.QFrame[frameShape="4"] {
  max-height: 2px;
  border: none;
  background-color: #6b7e6b;
}

.QFrame[frameShape="5"] {
  max-width: 2px;
  border: none;
  background-color: #6b7e6b;
}

/* ===================== QToolTip ======================== */

QToolTip {
  border: 1px solid #6b7e6b;
  padding: 2px;
  color: #fcfcfc;
  background-color: #383838;
}

/* ===================== QSplitter ======================== */

QSplitter {
  spacing: 0px;
  padding: 0px;
  margin: 0px;
}

QSplitter::handle {
  background-color: #456445;
  border: 0px solid #6b7e6b;
  spacing: 0px;
  padding: 1px;
  margin: 0px;
}

QSplitter::handle:hover {
  background-color: #9db59d;
}

QSplitter::handle:horizontal {
  width: 1px;
}

QSplitter::handle:vertical {
  height: 1px;
}

/* ===================== QGraphicsView ======================== */

QGraphicsView {
  background-color: #3a3a3a;
  border: 1px solid #6b7e6b;
  color: #ffffff;
  border-radius: 2px;
}

QGraphicsView:disabled {
  background-color: #3a3a3a;
  border: 1px solid #6b7e6b;
  color: #556455;
  border-radius: 2px;
}

QGraphicsView:hover, QGraphicsView:!hover, QGraphicsView:selected,
QGraphicsView:pressed {
  border: 1px solid #6b7e6b;
}

/* ===================== QToolBox ======================== */

QToolBox {
  padding: 0px;
  border: 0px;
  border: 1px solid #6b7e6b;
}

QToolBox:selected {
  padding: 0px;
  border: 1px solid #6b7e6b;
}

QToolBox::tab {
  border: 1px solid #6b7e6b;
  color: #fcfcfc;
  background-color: #525252;
}

QToolBox::tab:disabled {
  color: #9d9d9d;
}

QToolBox::tab:selected {
  background-color: #383838;
  border-bottom: 4px solid #509330;
}

QToolBox::tab:selected:disabled {
  background-color: #9d9d9d;
}

QToolBox::tab:!selected {
  background-color: #525252;
}

QToolBox::tab:!selected:disabled {
  background-color: #9d9d9d;
}

QToolBox::tab:hover {
  border-color: #6b7e6b;
  border-bottom: 2px solid #509330;
}

QToolBox QScrollArea QWidget QWidget {
  padding: 0px;
  border: 0px;
  background-color: #1b201b;
}

/* ===================== QDockWidget ======================== */

QDockWidget {
  outline: 1px solid #476444;
  background-color: #1b201b;
  border: 1px solid #6b7e6b;
  border-radius: 2px;
  titlebar-close-icon: url("${path}/dark_green/transparent.svg");

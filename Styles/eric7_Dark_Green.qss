QDockWidget::title {
  padding: 3px;
  spacing: 4px;
  border: none;
  background-color: #456445;
}

QDockWidget::close-button {
  icon-size: 12px;
  border: none;
  background: transparent;
  background-image: transparent;
  border: 0;
  margin: 0;
  padding: 0;
  image: url("${path}/dark_green/window_close.svg");
}

QDockWidget::close-button:hover {
  image: url("${path}/dark_green/window_close_focus.svg");
}

QDockWidget::close-button:pressed {

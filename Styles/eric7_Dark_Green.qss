  image: url("${path}/dark_green/checkbox_unchecked_pressed.svg");
}

QGroupBox::indicator:unchecked:disabled {
  image: url("${path}/dark_green/checkbox_unchecked_disabled.svg");
}

QGroupBox::indicator:checked {

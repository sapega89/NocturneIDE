/* ===================== QPalette ======================== */
/* == This entry is handled by eric == */
QPalette {
  alternate-base: #0f120f;
  base: #3a3a3a;
  text: #fcfcfc;
  bright-text: #ffffff;
  placeholder-text: #9d9d9d;
  window: #1b201b;
  window-text: #fcfcfc;
  tooltip-base: #383838;
  tooltip-text: #fcfcfc;
  button: #1b201b;
  button-text: #fcfcfc;
  highlight: #407540;
  highlighted-text: #e1e1e1;
  link: #c6ebc6;
  link-visited: #ebebc6
}
/* == End of eric specialties == */

* {
  outline: 0px;
  color: #fcfcfc;
  selection-background-color: #407540;
  selection-color: #ffffff;
}

/* specific reset for elements inside QToolBar */
QToolBar * {
  margin: 0px;
  padding: 0px;
}

QWidget, QStatusBar, QSvgWidget, QToolBar,
QGroupBox, QHelpSearchQueryWidget, QHelpSearchResultWidget,
QDesignerPropertyEditorInterface, QDesignerWidgetBoxInterface, QDesktopWidget,
QDialog, QDialogButtonBox, QDesignerActionEditorInterface,
QDesignerFormWindowInterface, QDesignerObjectInspectorInterface, 
QAbstractButton, QAbstractSlider, QAbstractSpinBox, QAxWidget, QCalendarWidget,
QComboBox, QMdiSubWindow, QMenu, QMenuBar,QDockWidget, QFocusFrame, QGLWidget,
QToolBox, QLCDNumber , QSplitter, QStackedWidget, QMainWindow,
QScrollArea, QAbstractScrollArea {
    background-color: #1b201b;
}

QWidget:disabled {
  background-color: #1b201b;
  color: #9d9d9d;
  selection-background-color: #407540;
  selection-color: #e1e1e1;
}

QWidget::item:selected {
  background-color: #407540;
}

/* ===================== QMainWindow ======================== */

QMainWindow::separator {
  background-color: #456445;
  border: 0px solid #6b7e6b;
  spacing: 0px;
  padding: 1px;
}

QMainWindow::separator:hover {
  background-color: #9db59d;
}

QMainWindow::separator:horizontal {
  width: 5px;
  margin-top: 2px;
  margin-bottom: 2px;
  image: url("${path}/dark_green/toolbar_separator_vertical.svg");
}

QMainWindow::separator:vertical {
  height: 5px;
  margin-left: 2px;
  margin-right: 2px;
  image: url("${path}/dark_green/toolbar_separator_horizontal.svg");
}

/* ===================== QComboxBox ======================== */

QLineEdit, QTextEdit, QPlainTextEdit, QSpinBox, QDoubleSpinBox, QComboBox,
QDateEdit, QTimeEdit, QDateTimeEdit {
  background-color: #3a3a3a;
  border: 1px solid #6b7e6b;
  border-radius: 4px;
  padding-left: 4px;
  padding-right: 4px;
  min-height: 1em;
}

QComboBox::indicator {
  border: none;
  border-radius: 0;
  border-width: 0px;
  color: transparent;
  background-color: transparent;
  selection-color: transparent;
  selection-background-color: transparent;
}

QComboBox::indicator:alternate {
  background: #0f120f;
}

QComboBox::indicator:checked {
  image: url("${path}/dark_green/combobox_checked.svg");
}

QComboBox::drop-down, QDateEdit::drop-down, QTimeEdit::drop-down,
QDateTimeEdit::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 15px;
  border-left-width: 0px;
  border-left-style: solid;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
  border-left-color: #242424;
}

QComboBox::drop-down:on {
  top:1px;
}

QComboBox::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 20px;
  border-left: 1px solid #6b7e6b;
}

QComboBox::down-arrow {
  image: url("${path}/dark_green/arrow_down_disabled.svg");
  height: 14px;
  width: 14px;
}

QComboBox::down-arrow:on,
QComboBox::down-arrow:hover,
QComboBox::down-arrow:focus {
  image: url("${path}/dark_green/arrow_down.svg");
}

QComboBox::separator {
  height: 2px;
  background-color: #fcfcfc;
}

QLineEdit[text=""] {
  color: 9d9d9d;
}

/* ===================== QMenuBar ======================== */

QMenuBar * {
  background-color: #252c25;
}

QMenuBar::item {
  color: #fcfcfc;
  background-color: #1b201b;
  margin: 1px;
  padding: 4px;
}

QMenuBar::item:selected {
  padding: 4px;
  background: transparent;
  border: 0px solid #6b7e6b;
  background-color: #407540;
}

QMenuBar::item:pressed {
  padding: 4px;
  border: 0px solid #6b7e6b;
  background-color: #407540;
  color: #fcfcfc;
  margin-bottom: 0px;
  padding-bottom: 0px;
}

/* ===================== QMenu ======================== */

QMenu {
  background-color: #252c25;
  border: 1px solid #6b7e6b;
  margin: 2px;
  padding: 5px;
}

QMenu * {
  background-color:#252c25;
}

QMenu::tearoff {
  border-top: 2px dashed #6b7e6b;
  border-bottom: 2px dashed #6b7e6b;
  height: 0px;
  margin-top: 3px;
  margin-bottom: 3px;
}

QMenu::tearoff:selected {
  border-top: 2px dashed #bbdcbb;
  border-bottom: 2px dashed #bbdcbb;
  background: #407540;
  background-clip: margin;
}

QMenu::item:selected {
  color: #fcfcfc;
  border: 0px solid #6b7e6b;
  background: #407540;
}

QMenu::item:checked {
  color: #fcfcfc;
  border: 1px solid #407540;
  border-radius: 4px;
  background: #2a322a;
}

QMenu::separator {
  height: 2px;
  background: #6b7e6b;
  margin: 5px;
}

QMenu::indicator {
  width: 14px;
  height: 14px;
  spacing: 4px
}

/* non-exclusive indicator = check box style indicator (see
   QActionGroup::setExclusive) */

QMenu::indicator:non-exclusive:unchecked {
  image: url("${path}/dark_green/checkbox_unchecked.svg");
}

QMenu::indicator:non-exclusive:unchecked:hover,
QMenu::indicator:non-exclusive:unchecked:focus,
QMenu::indicator:non-exclusive:unchecked:pressed {
  border: none;
  image: url("${path}/dark_green/checkbox_unchecked_focus.svg");
}

QMenu::indicator:non-exclusive:unchecked:disabled {
  image: url("${path}/dark_green/checkbox_unchecked_disabled.svg");
}

QMenu::indicator:non-exclusive:checked {
  image: url("${path}/dark_green/checkbox_checked.svg");
}

QMenu::indicator:non-exclusive:checked:hover,
QMenu::indicator:non-exclusive:checked:focus,
QMenu::indicator:non-exclusive:checked:pressed {
  border: none;
  image: url("${path}/dark_green/checkbox_checked_focus.svg");
}

QMenu::indicator:non-exclusive:checked:disabled {
  image: url("${path}/dark_green/checkbox_checked_disabled.svg");
}

QMenu::indicator:non-exclusive:indeterminate {
  image: url("${path}/dark_green/checkbox_indeterminate.svg");
}

QMenu::indicator:non-exclusive:indeterminate:disabled {
  image: url("${path}/dark_green/checkbox_indeterminate_disabled.svg");
}

QMenu::indicator:non-exclusive:indeterminate:focus,
QMenu::indicator:non-exclusive:indeterminate:hover,
QMenu::indicator:non-exclusive:indeterminate:pressed {
  image: url("${path}/dark_green/checkbox_indeterminate_focus.svg");
}

/* exclusive indicator = radio button style indicator (see
   QActionGroup::setExclusive) */

QMenu::indicator:exclusive:unchecked {
  image: url("${path}/dark_green/radio_unchecked.svg");
}

QMenu::indicator:exclusive:unchecked:hover,
QMenu::indicator:exclusive:unchecked:focus,
QMenu::indicator:exclusive:unchecked:pressed {
  border: none;
  outline: none;
  image: url("${path}/dark_green/radio_unchecked_focus.svg");
}

QMenu::indicator:exclusive:unchecked:disabled {
  image: url("${path}/dark_green/radio_unchecked_disabled.svg");
}

QMenu::indicator:exclusive:checked {
  border: none;
  outline: none;
  image: url("${path}/dark_green/radio_checked.svg");
}

QMenu::indicator:exclusive:checked:hover,
QMenu::indicator:exclusive:checked:focus,
QMenu::indicator:exclusive:checked:pressed {
  border: none;
  outline: none;
  image: url("${path}/dark_green/radio_checked_focus.svg");
}

QMenu::indicator:exclusive:checked:disabled {
  outline: none;
  image: url("${path}/dark_green/radio_checked_disabled.svg");
}

QMenu::right-arrow {
  image: url("${path}/dark_green/arrow_right.svg");
  margin: 5px;
  padding-left: 12px;
  height: 10px;
  width: 10px;
}

QMenu::right-arrow:disabled {
  image: url("${path}/dark_green/arrow_right_disabled.svg");
}
/* ===================== QProgressBar ======================== */

QProgressBar {
  min-height: 10px;
  background: #3a3a3a;
  border-radius: 1px;
  text-align: center;
  border: 1px solid #3a3a3a;
  border-radius: 4px;
}

QProgressBar:chunk {
  background-color:#467e2a;
  border-radius: 4px;
}

/* ===================== QTabBar ======================== */

QTabBar::tab {
  border: 1px solid #242424;
  color: #fcfcfc;
  margin: 0px;
  background: qlineargradient(
    spread: pad, x1: 0, y1: 0, x2: 0, y2: 1,
    stop: 0 #646464, stop: 1 #525252);
}

QTabBar::tab:selected, QTabBar::tab:hover {
  border-style: solid;
  border-color: #509330;
  background: qlineargradient(
    spread: pad, x1: 0, y1: 0, x2: 0, y2: 1,
    stop: 0 #484848, stop: 1 #383838);
}

QTabBar::tab:top, QTabBar::tab:bottom {
  padding: 3px 8px 3px 8px;
}

QTabBar::tab:left, QTabBar::tab:right {
  padding: 8px 3px 8px 3px;
}

QTabBar::tab:top:selected {
  border-width: 4px 0px 0px 0px;
}

QTabBar::tab:right:selected {
  border-width: 0px 0px 0px 4px;
}

QTabBar::tab:bottom:selected {
  border-width:0px 0px 4px 0px;
}

QTabBar::tab:left:selected {
  border-width: 0px 4px 0px 0px;
}

QTabBar::tab:top:hover {
  border-width: 2px 0px 0px 0px;
}

QTabBar::tab:right:hover {
  border-width: 0px 0px 0px 2px;
}

QTabBar::tab:bottom:hover {
  border-width:0px 0px 2px 0px;
}

QTabBar::tab:left:hover {
  border-width: 0px 2px 0px 0px;
}

QTabBar::tab:first:top:selected, QTabBar::tab:first:top:hover,
QTabBar::tab:first:bottom:selected, QTabBar::tab:first:bottom:hover {
  border-left-width:1px;
  border-left-color:#242424;
}

QTabBar::tab:first:left:selected, QTabBar::tab:first:left:hover,
QTabBar::tab:first:right:selected, QTabBar::tab:first:right:hover {
  border-top-width:1px;
  border-top-color:#242424;
}

QTabBar::tab:last:top:selected, QTabBar::tab:last:top:hover,
QTabBar::tab:last:bottom:selected, QTabBar::tab:last:bottom:hover {
  border-right-width:1px;
  border-right-color:#242424;
}

QTabBar::tab:last:left:selected, QTabBar::tab:last:left:hover,
QTabBar::tab:last:right:selected, QTabBar::tab:last:right:hover {
  border-bottom-width:1px;
  border-bottom-color:#242424;
}

QTabBar QToolButton::left-arrow:enabled,
QDockWidget QTabBar QToolButton::left-arrow:enabled {
  image: url("${path}/dark_green/arrow_left.svg");
}

QTabBar QToolButton::left-arrow:disabled,
QDockWidget QTabBar QToolButton::left-arrow:disabled {
  image: url("${path}/dark_green/arrow_left_disabled.svg");
}

QTabBar QToolButton::right-arrow:enabled,
QDockWidget QTabBar QToolButton::right-arrow:enabled {
  image: url("${path}/dark_green/arrow_right.svg");
}

QTabBar QToolButton::right-arrow:disabled,
QDockWidget QTabBar QToolButton::right-arrow:disabled {
  image: url("${path}/dark_green/arrow_right_disabled.svg");
}

/* ===================== QTabWidget ======================== */

QTabWidget >* {
    background-color: #252c25;
}

QTabWidget::pane {
  border:1px solid #6b7e6b;
  alternate-background-color:#0f120f;
  gridline-color:#242424;
  background:#3a3a3a;
}
QTabWidget::pane:selected {
  background-color: #455364;
  border: 1px solid #346792;
}

/* ===================== QStatusBar ======================== */

QStatusBar::item{
  border:0px solid #484848;
  border-radius:3px;
}

/* ===================== QHeaderView ======================== */

QHeaderView {
  border: 0px transparent #6b7e6b;
}

QHeaderView::section {
  background-color: #5a6b5a;
  color: #fcfcfc;
  border-radius: 0;
  text-align: left;
  font-size: 13px;
}

QHeaderView::section::horizontal {
  padding-top: 0;
  padding-bottom: 0;
  padding-left: 4px;
  padding-right: 4px;
  border-left: 2px solid #242424;
}

QHeaderView::section::vertical {
  padding-top: 0;
  padding-bottom: 0;
  padding-left: 4px;
  padding-right: 4px;
  border-top: 2px solid #242424;
}

QHeaderView[sortable="true"]::down-arrow {
  background-color: #5a6b5a;
  border: none;
  height: 10px;
  width: 10px;
  padding-left: 2px;
  padding-right: 2px;
  image: url("${path}/dark_green/arrow_down.svg");
}

QHeaderView[sortable="true"]::up-arrow {
  background-color: #5a6b5a;
  border: none;
  height: 10px;
  width: 10px;
  padding-left: 2px;
  padding-right: 2px;
  image: url("${path}/dark_green/arrow_up.svg");
}

/* ===================== QTreeView ======================== */

QTreeView:branch:has-children:!has-siblings:closed,
QTreeView:branch:closed:has-children:has-siblings {
  border-image: none;
  image: url("${path}/dark_green/branch_closed.svg");
}

QTreeView:branch:open:has-children:!has-siblings,
QTreeView:branch:open:has-children:has-siblings {
  border-image: none;
  image: url("${path}/dark_green/branch_open.svg");
}

QTreeView:branch:has-children:!has-siblings:closed:hover,
QTreeView:branch:closed:has-children:has-siblings:hover {
  image: url("${path}/dark_green/branch_closed_focus.svg");
}

QTreeView:branch:open:has-children:!has-siblings:hover,
QTreeView:branch:open:has-children:has-siblings:hover {
  image: url("${path}/dark_green/branch_open_focus.svg");
}

QTreeView,
QListView,
QTableView,
QColumnView {
  background-color: #1b201b; 
  alternate-background-color: #0f120f;
  border: 1px solid #6b7e6b;
  gridline-color: #6b7e6b;
}

QTreeView QLabel {
  background-color: transparent; 
}

QTreeView::item:selected,
QListView::item:selected,
QTableView::item:selected,
QColumnView::item:selected{

/* ===================== QAbstractSpinBox ======================== */

QAbstractSpinBox {
  background-color: #3a3a3a;
  border: 1px solid #6b7e6b;
  color: #e0e3e1;
  padding-top: 2px;
  padding-bottom: 2px;
  padding-left: 4px;
  padding-right: 4px;
  border-radius: 4px;
}

QAbstractSpinBox:up-button {
  background-color: #1b201b;
  subcontrol-origin: border;
  subcontrol-position: top right;
  border: 1px solid #6b7e6b;
  border-bottom: 1px solid #6b7e6b;
  border-top-left-radius: 0;
  border-top-right-radius: 4;
  border-bottom-left-radius: 0;
  width: 12px;
  margin-bottom: -1px;
}

QAbstractSpinBox:down-button {
  background-color: #1b201b;
  subcontrol-origin: border;
  subcontrol-position: bottom right;
  border: 1px solid #6b7e6b;
  border-top: 1px solid #6b7e6b;
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
  border-bottom-right-radius: 4;
  width: 12px;
  margin-top: -1px;
}

QAbstractSpinBox::up-arrow {
  image: url("${path}/dark_green/arrow_up.svg");
  height: 10px;
  width: 10px;
}

QAbstractSpinBox::up-arrow:disabled,
QAbstractSpinBox::up-arrow:off {
  image: url("${path}/dark_green/arrow_up_disabled.svg");
}

QAbstractSpinBox::down-arrow {
  image: url("${path}/dark_green/arrow_down.svg");
  height: 10px;
  width: 10px;
}

QAbstractSpinBox::down-arrow:disabled,
QAbstractSpinBox::down-arrow:off {
  image: url("${path}/dark_green/arrow_down_disabled.svg");
}

/* ===================== QToolButton ======================== */

QToolButton {
  background-color: #2a322a;
  border: 1px solid #6b7e6b;
  border-radius: 4px;
  padding-top: 2px;
  padding-bottom: 2px;
  padding-left: 4px;
  padding-right: 4px
}

QToolButton:checked {
  background-color: #566656;
  border: 1px solid #6b7e6b;
}

QToolButton:checked:disabled {
  background-color: #566656;
  color: #9d9d9d;
  border: 1px solid #6b7e6b;
}


QToolButton:hover {
  background-color: #404c40;
}

QToolButton:pressed {
  background-color: #202620;
  border: 1px solid #6b7e6b;
}

QToolButton[popupMode="0"] {
  /* Only for DelayedPopup */
  padding-right: 2px;
}

QToolButton[popupMode="1"] {
  /* Only for MenuButtonPopup */
  padding-right: 20px;
}

QToolButton[popupMode="1"]::menu-button {
  border: none;
}

QToolButton[popupMode="1"]::menu-button:hover {
  border: none;
  border-left: 1px solid #6b7e6b;
  border-radius: 0;
}

QToolButton[popupMode="2"] {
  /* Only for InstantPopup */
  padding-right: 2px;
}

QToolButton::menu-button {
  image: url("${path}/dark_green/arrow_down.svg");
  padding: 2px;
  border-radius: 4px;
  width: 12px;
  border: 1px solid #6b7e6b;
  outline: none;
}

QToolButton::menu-button:hover {
  border: 1px solid #539236;
}

QToolButton::menu-button:checked:hover {
  border: 1px solid #539236;
}

QToolButton::menu-indicator {
  image: url("${path}/dark_green/arrow_down.svg");
  height: 6px;
  width: 6px;
  top: 0;
  /* Exclude a shift for better image */
  left: -2px;
  /* Shift it a bit */
}

QToolButton::menu-arrow {
  image: url("${path}/dark_green/arrow_down.svg");
  height: 8px;
  width: 8px;
}

QToolButton::menu-arrow:hover {
  image: url("${path}/dark_green/arrow_down_focus.svg");
}

/* ===================== QPushButton ======================== */

QPushButton {
  background-color: #2a322a;
  border: 1px solid #6b7e6b;
  border-radius: 4px;
  padding-top: 2px;
  padding-bottom: 2px;
  padding-left: 4px;
  padding-right: 4px
}

QPushButton:default {
  border: 2px solid #467e2a;
}

QPushButton:checked {
  background-color: #566656;
  border: 1px solid #6b7e6b;
}

QPushButton:checked:disabled {
  background-color: #566656;
  color: #9d9d9d;
  border: 1px solid #6b7e6b;
}

QPushButton:hover {
  background-color: #404c40;
}

QPushButton:pressed {
    background-color: #202620;
    border: 1px solid #6b7e6b;
}

QPushButton::menu-indicator {
  subcontrol-origin: padding;
  subcontrol-position: bottom right;
  bottom: 4px;
}

QDialogButtonBox QPushButton {
  min-width: 80px;
}

/* ===================== QSizeGrip ======================== */

QSizeGrip {
  background: transparent;
  width: 20px;
  height: 20px;
  image: url("${path}/dark_green/window_grip.svg");
}

/* ===================== QToolBar ======================== */

QToolBar {
  border-bottom: 1px solid #6b7e6b;
  padding: 1px;
  font-weight: bold;
  spacing: 2px;
}

QToolBar::handle:horizontal {
  width: 16px;
  image: url("${path}/dark_green/toolbar_move_horizontal.svg");
}

QToolBar::handle:vertical {
  height: 16px;
  image: url("${path}/dark_green/toolbar_move_vertical.svg");
}

QToolBar::separator:horizontal {
  width: 8px;
  image: url("${path}/dark_green/toolbar_separator_horizontal.svg");
}

QToolBar::separator:vertical {
  height: 8px;
  image: url("${path}/dark_green/toolbar_separator_vertical.svg");
}

QToolButton#qt_toolbar_ext_button {
  image: url("${path}/dark_green/arrow_right.svg");
}

QToolBar QToolButton, QToolButton:pressed {
  border: none
}

/* ===================== QCommandLinkButton ======================== */

QCommandLinkButton {
  background-color: transparent;
  border: 1px solid #6b7e6b;
  border-radius: 4px;
  padding: 0px;
  margin: 0px;
}

QCommandLinkButton:disabled {
  background-color: transparent;
  color: #9db5a9;
}
